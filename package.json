{
  "name": "uk-traffic",
  "version": "1.1.1",
  "description": "A Node module to provide access to UK traffic information",
  "main": "index.ts",
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "watch": "tsc --project tsconfig.json -w",
    "build": "parcel build index.ts --target=node --bundle-node-modules",
    "test:lint": "eslint index.ts",
    "test:unit": "jest src",
    "test:e2e": "jest index.test.js",
    "pretest": "npm run build",
    "test": "npm run test:unit && npm run test:e2e && npm run test:lint",
    "preversion": "npm test",
    "postversion": "git push --follow-tags && npm publish"
  },
  "repository": "git@github.com:AlexChesters/traffic.git",
  "author": "Alex Chesters <alex.chesters@bbc.co.uk>",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "rss-url-parser": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/types": "^7.10.3",
    "babel-jest": "^26.1.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "jest-json-schema": "^2.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-typescript": "^1.0.0",
    "typescript": "^3.9.5"
  }
}
